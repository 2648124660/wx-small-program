.navigator {
    display: flex;
    flex-direction: row;
    justify-content: center;
    width: 100%;
    height: 100rpx;
    align-items: center;
    background: white;
    border-bottom:1rpx solid #eeeeee;
}

.action-sheet {
    background: white;
}

.action-sheet-box {
    position: absolute;
    top: 200rpx;
    left: 60rpx;
    right: 60rpx;
    z-index: 2;
}

.choose-cate {
    width: 100%;
    display: flex;
    flex-direction: row;
    line-height: 80rpx;
    max-height: 80rpx;
    overflow: hidden
}

.place-holder {
    color: #ccc
}

